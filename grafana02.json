      "title": "CPU – % Processor Time (_Total)",
      "datasource": "$influxdb",
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 1 },
      "targets": [
        {
          "refId": "A",
          "rawQuery": true,
          "resultFormat": "time_series",
          "query": "SELECT last(\"value\") FROM \"azure_perf_counter\" WHERE $timeFilter AND \"object_name\" = 'Processor' AND \"counter_name\" = '% Processor Time' AND \"instance_name\" = '_Total' AND \"computer\" =~ /^$computer$/ GROUP BY \"computer\""
        }
      ],
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"],
          "fields": "",
          "values": false
        },
        "orientation": "horizontal",
        "colorMode": "value",
        "graphMode": "area",
        "justifyMode": "center"
      },
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "decimals": 1,
          "min": 0,
          "max": 100,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 70 },
              { "color": "red", "value": 85 }
